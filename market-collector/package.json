{
  "name": "market-collector",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "scripts": {
    "start": "nodemon -e ts,js --exec 'ts-node -r tsconfig-paths/register' ./src/index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "ext": "ts,js",
    "delay": 2500
